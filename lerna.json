{
  "version": "independent",
  "useWorkspaces": true,
  "command": {
    "version": {
      "allowBranch": "master",
      "conventionalCommits": true,
      "noPrivate": true,
      "message": "chore: Publish :shipit: [skip ci]"
    }
  },
  "ignoreChanges": [
    "package-lock.json",
    "Readme.md",
    "Changelog.md",
    "CHANGELOG.md",
    "*.spec.js",
    "*.snap"
  ]
}
